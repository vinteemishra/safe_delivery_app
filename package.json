    "react-native-reanimated": "^1.4.0",
    "react-native-render-html": "^4.1.2",
    "react-native-restart": "0.0.10",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-safe-area-view": "^0.14.7",
