{
  "name": "safedelivery",
  "version": "3.3.8",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 15 Pro'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "adbLogcat": "adb shell 'logcat --pid=$(pidof -s dk.maternity.safedelivery) | grep --line-buffered -v EGL_emulation'",
    "adbClear": "adb shell pm clear dk.maternity.safedelivery",
    "adbStart": "adb shell am start -n dk.maternity.safedelivery/dk.maternity.safedelivery.MainActivity",
    "adbRestart": "npm run adbClear && npm run adbStart && npm run adbLogcat",
    "postinstall": "rm -rf node_modules/react-native-restart/.git/ || true"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "dependencies": {
    "@dudigital/react-native-zoomable-view": "^1.1.3",
